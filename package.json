  "author": "Boyan Mihaylov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/boyanio/react-blazor.git"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "connect": "^3.6.6",
    "connect-modrewrite": "^0.10.2",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.2",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
